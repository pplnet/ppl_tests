test("repetir base", -> {
    &count := 1
    Repetir
        If &count = 3
            Break
        EndIf
        &count := &count + 1
    Proximo
    assertEq(3, &count)
})

#ifdef V6
test("repetir cancelar", -> {
    try
       Repetir
         Cancelar
         Break
       Proximo
       ACN(Z:1,123)
    catch
       assertEq(NULL, Val(Z:1))
    end
})
#endif