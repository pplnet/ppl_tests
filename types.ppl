** Tests para verificar los tipos de datos

let(&string1, 'prueba1')
let(&string2, 'prueba2')
let(&fechaPplObj, Fecha('02/01/2020')) 
let(&fechaDateTime, DateFromStr('02/01/2020'))

test('types', -> {
  assertEq('Int32', GetType(1))
  assertEq('Int32', GetType(0))
  assertEq('Int32', GetType(1 + 1))
  assertEq('Int32', GetType(-10 + 1))
  assertEq('Int32', GetType(-10 - 1))
  assertEq('Int32', GetType(Int(1)))
  assertEq('String', GetType('Hola'))
  assertEq('String', GetType(Copy('Hola',2,2)))
  assertEq('String', GetType(&string1))
  assertEq('String', GetType(&string1~&string2))
  assertEq('String', GetType(''))
  assertEq('String', GetType(""))
  assertEq('String', GetType("''"))
  assertEq('String', GetType("Hola"))
  assertEq('String', GetType(FStr(100,0,0)))
  assertEq('String', GetType(FStr(123.55,0,2)~FStr(123.55,0,2)))
  assertEq('String', GetType(Str('Hola')))
  assertEq(Null, GetType(Null))
  assertEq('DateTime', GetType(Fecha('02/01/2020')))
  assertEq('DateTime', GetType(Fecha('02/01/2020') - 1))
  assertEq('DateTime', GetType(NewPplObj(Fecha('02/01/2020'))))
  assertEq('DateTime', GetType(&fechaPplObj))
  assertEq('DateTime', GetType(DateFromStr('02/01/2020')))
  assertEq('DateTime', GetType(&fechaDateTime))
  assertEq('DateTime', GetType(Ahora))
  assertEq('DateTime', GetType(Dat(Ahora)))
  assertEq('Double', GetType(Num('123')))
  assertEq('Double', GetType(Num('123') + 10))
  assertEq('Double', GetType(1.123456))
  assertEq('Double', GetType(2 + 1.123456))
  assertEq('Double', GetType(2 - 1.123456))
  assertEq('Double', GetType(-1.123456))
  assertEq('Double', GetType(Dbl(1.123456)))
  assertEq('Boolean', GetType(SI))
  assertEq('Boolean', GetType(NO))
  assertEq('Boolean', GetType(1=1))
  assertEq('Boolean', GetType(0=1))
  assertEq('Boolean', GetType(0>1))
  assertEq('Boolean', GetType(0>=1))
  assertEq('Boolean', GetType(0<=1))
  assertEq('Boolean', GetType(Bln(SI)))
  assertEq('Decimal', GetType(Decimal(2)))
})
