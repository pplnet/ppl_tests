test("recorrer_sql", ->{
    sql.addrow(${Col1 = 1})
    sql.addrow(${Col1 = 2})
    &idx := 0
    recorrer sql
        &idx := &idx + 1
        assertEq(&idx, fbn("Col1"))
    proximo
})

