test('getter', -> { 
    &foo := '123'
    assertEq(3, &foo.get_length)
})

test('setter', -> { 
    &foo := NewGetSet()
    &foo.set_prop(1)
    assertEq(1, &foo.get_prop)
})

if(Version() = 6)
   Cancelar
endif

test('getset embebido', -> { 
    &foo := NewGetSet()
    &bar := NewGetSet()
    &bar.set_prop(2)
    &foo.set_prop(&bar)
    assertEq(2, &foo.get_prop.get_prop)
})