def concat(arg1, arg2)
   return arg1~arg2
end

def ppl_echo(msg)
   return msg
end

def to_string(value)
   return value.ToString()
end

test("def func args", -> {
    assertEq('HolaMundo', concat('Hola', 'Mundo'))
    assertEq('Hello pipex!', ppl_echo('Hello pipex!'))
    assertEq('1', to_string(1))
})

