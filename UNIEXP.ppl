&num := 3.5
&cero := 0
&uno := 1

test('expresiones unarias', -> {
  assertEq(-1.1, -1.1)
  assertEq( 1.1, -(-1.1))
  assertEq(-3.5, -&num)
  assertEq( 3.5, -(-&num))
  
  assertEq(+1.1, +1.1)
  assertEq(-1.1, +(-1.1))
  assertEq(+3.5, +&num)
  assertEq(-3.5, +(-&num))
  
  assertEq(NO, not SI)
  assertEq(SI, not NO)
  assertEq(NO, not 1)
  assertEq(SI, not 0)
  assertEq(SI, not &cero)
  assertEq(NO, not &uno)
})
