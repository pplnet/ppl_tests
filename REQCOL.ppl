test("recorrer col", ->{
    RecorrerColumnas 1,3
      ACN(colciclo:1, colciclo)
    Proximo
    assertEq(1, Val(A:1))
    assertEq(2, Val(B:1))
    assertEq(3, Val(C:1))
    
    RecorrerCol 1,3
      ACN(colciclo:1, colciclo)
    Proximo
    assertEq(1, Val(A:1))
    assertEq(2, Val(B:1))
    assertEq(3, Val(C:1))
    
    ACN(A:1,0)
    ACN(B:1,0)
    ACN(C:1,0)
    RecorrerCol 1,3
      ACN(colciclo:1, colciclo)
      Break
    Proximo
    assertEq(1, Val(A:1))
    assertEq(0, Val(B:1))
    assertEq(0, Val(C:1))
    
    ACN(A:1,0)
    ACN(B:1,0)
    ACN(C:1,0)
    RecorrerCol 1,3
      ACN(colciclo+1:1, colciclo)
    Proximo
    assertEq(0, Val(A:1))
    assertEq(1, Val(B:1))
    assertEq(2, Val(C:1))
    
    ACN(Z:1,3)
    RecorrerCol 1,Val(Z:1)
      ACN(colciclo:1, colciclo)
    Proximo
    assertEq(1, Val(A:1))
    assertEq(2, Val(B:1))
    assertEq(3, Val(C:1))
    
    RecorrerCol 1,Val(Z:1)+1
      ACN(colciclo:1, colciclo)
    Proximo
    assertEq(1, Val(A:1))
    assertEq(2, Val(B:1))
    assertEq(3, Val(C:1))
    assertEq(4, Val(D:1))
    
    RecorrerCol 1,3
      ACN(colciclo:1, colciclo)
    Proximo
    assertEq(6, Val(A:1)+Val(B:1)+Val(C:1))
})
